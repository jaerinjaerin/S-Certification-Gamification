{
  "name": "splus-cert-gamification",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cp -f .env.dev .env && next dev -p 3000",
    "local": "cp -f .env.loc .env && next dev -p 3000",
    "stage": "cp -f .env.stage .env && next dev -p 3000",
    "build:stg": "NODE_ENV=staging && rm -rf .next && next build",
    "build:prd": "NODE_ENV=production && rm -rf app/(client)/home && rm -rf app/(client)/test && rm -rf .next && next build",
    "build:local": "cp -f .env.loc.production .env && next build",
    "build:analyze": "npm run build && source-map-explorer .next/static/**/*.js",
    "analyze": "ANALYZE=true next build",
    "start": "next start -p 3000",
    "start:local": "cp -f .env.loc.production .env && next start -p 3000",
    "lint": "next lint",
    "test": "jest",
    "postinstall": "patch-package",
    "test:watch": "jest --watch",
    "prisma:generate": "PRISMA_CLIENT_OUTPUT=../../client/node_modules/.prisma/client npx prisma generate --schema=../data-model/prisma/schema.prisma",
    "prisma:studio": "PRISMA_CLIENT_OUTPUT=../../client/node_modules/.prisma/client npx prisma studio --schema=../data-model/prisma/schema.prisma",
    "pm2:stop": "pm2 delete squiz",
    "pm2:start": "pm2 start npm --name 'squiz' -- start",
    "pm2:go": "pm2 start ecosystem.config.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.3",
    "@aws-sdk/client-ses": "^3.696.0",
    "@lottiefiles/dotlottie-react": "^0.12.0",
    "@next/third-parties": "^15.1.4",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-email/components": "^0.0.27",
    "@sentry/nextjs": "^8.47.0",
    "auto-text-size": "^0.2.3",
    "aws-sdk": "^2.1692.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cuid": "^3.0.0",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.5.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.468.0",
    "motion": "^11.13.5",
    "next": "14.1.0",
    "next-auth": "5.0.0-beta.4",
    "next-intl": "^3.25.3",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18",
    "react-dom": "18",
    "react-email": "^3.0.2",
    "resend": "^4.0.1-alpha.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "usehooks-ts": "^3.1.0",
    "uuid": "^11.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prisma-erd-generator": "^1.11.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
