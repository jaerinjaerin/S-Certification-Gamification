{
  "name": "dashboard",
  "version": "25.06.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "local": "cp -f .env.loc .env && next dev -p 3000",
    "stage": "cp -f .env.stage .env && next dev -p 3000",
    "build": " rm -rf .next && next build",
    "build:local": "cp -f .env.loc.production .env && next build",
    "build:analyze": "npm run build && source-map-explorer .next/static/**/*.js",
    "analyze": "ANALYZE=true next build",
    "start": "next start -p 3000",
    "start:local": "cp -f .env.loc.production .env && next start -p 3000",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:generate": "npx prisma generate --schema=../data-model/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=../data-model/prisma/schema.prisma",
    "pm2:stop": "pm2 delete squiz",
    "pm2:start": "pm2 start npm --name 'squiz' -- start"
  },
